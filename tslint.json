{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.*", "dist/**/*.*", "coverage/**/*.*"]
  },
  "jsRules": {},
  "rules": {
    "prettier": true,
    "no-namespace": false,
    "max-classes-per-file": false,
    "member-ordering": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
